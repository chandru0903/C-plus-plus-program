#include<iostream>
using namespace std;
int main(){
    int n,m;
    cout<<"Enter the number of elements in the first array : ";
    cin>>n;
    int ar1[n];
    cout<<"Enter the elements : "<<endl;
    for(int i=0;i<n;i++){
        cin>>ar1[i];
    }
    cout<<"Enter the number of elements for second array : ";
    cin>>m;
    int ar2[m];
    cout<<"Enter the elements : "<<endl;
    for(int i=0;i<m;i++){
        cin>>ar2[i];
    }
    int x=n+m;
    int ar3[x];
    for(int i=0;i<n;i++){
        ar3[i]=ar1[i];
    }
    for(int i=0;i<m;i++){
        ar3[n+i]=ar2[i];
    }
    
    for(int i=0;i<x;i++){
        cout<<ar3[i]<<" ";
    }
}
