#include<iostream>
using namespace std;
int main(){
    int n=3,m=4,a=1;
    int mat[n][m];
    
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            mat[i][j]=a;
            a+=1;}
    }
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            cout<<mat[i][j]<<" ";
            }
        cout<<endl;
    }
    
}
